{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "package": {
    "productName": "Tauri Vite Nest Prisma Example",
    "version": "0.1.0"
  },
  "build": {
    "beforeBuildCommand": "pnpm run build:web",
    "beforeDevCommand": "pnpm run dev:web",
    "devPath": "http://localhost:5173",
    "distDir": "../apps/web/dist"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "externalBin": [
        "binaries/my-backend"
      ],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri-vite-nest-prisma-example.app",
      "longDescription": "",
      "resources": [],
      "shortDescription": "",
      "targets": [
        "msi",
        "nsis"
      ],
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": false,
      "shell": {
        "sidecar": true
      },
      "http": {
        "all": true,
        "request": true
      }
    },
    "windows": [
      {
        "title": "Tauri Vite Nest Prisma Example",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}
